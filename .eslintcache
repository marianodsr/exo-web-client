ners/ProductView.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/LoginForm.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/Navbar.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/ProductCard.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/TagCarrousel.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/CloudImage.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/EditableValueRow.js",["160","161"],"import React, { useState, useRef, useEffect } from \"react\";\r\nimport EditButton from \"./EditButton\";\r\nimport ConfirmDiscardPair from \"./ConfirmDiscardPair\";\r\n\r\nconst EditableValueRow = ({\r\n  confirmCallback,\r\n  label,\r\n  value,\r\n  pre = null,\r\n  post = null,\r\n  rowColor = \"bg-gray-200\",\r\n  textColor = \"text-secondary\",\r\n  buttonColor = \"bg-primary\",\r\n}) => {\r\n  \r\n  const [stateValue, setStateValue] = useState(value);\r\n  const [editValue, setEditValue] = useState(stateValue);\r\n  const [editMode, setEditMode] = useState(false);\r\n\r\n  const onConfirm = async () => {\r\n    try {\r\n      await confirmCallback(editValue);\r\n      setStateValue(editValue);\r\n      setEditMode(false)\r\n    } catch (e) {\r\n      setEditValue(value)\r\n      setEditMode(false);\r\n    }\r\n  };\r\n\r\n  const onDiscard = () => {\r\n    setEditValue(stateValue);\r\n\r\n    setEditMode(false);\r\n  };\r\n\r\n\r\n\r\n  const onEditClick = () => {\r\n    setEditMode(true)\r\n  }\r\n\r\n  return (\r\n    <>\r\n      <div\r\n        className={`${textColor}  ${rowColor} font-bold text-xl flex items-center justify-center py-2`}\r\n      >\r\n        <h3>{label}</h3>\r\n      </div>\r\n      <div\r\n        className={`${textColor}  ${rowColor} font-bold text-xl flex items-center justify-center py-2`}\r\n      >\r\n        {!editMode ? (\r\n          <h3 className=\"flex space-x-1\">{pre && <pre className=\"font-bold\">{pre}</pre>}{stateValue}{post && <span className=\"font-bold\">{post}</span> }</h3>\r\n        ) : (\r\n          <input\r\n            type=\"text\"\r\n            className=\"w-3/4 text-center px-1 font-bold text-xl\"\r\n            value={editValue}\r\n            onChange={(e) => setEditValue(e.target.value)}\r\n            ref={input => input && input.focus()}\r\n          />\r\n        )}\r\n      </div>\r\n      <div\r\n        className={`${rowColor} flex items-center justify-center text-xl font-bold outline:none`}\r\n      >\r\n        {!editMode ? (\r\n          <EditButton\r\n            className={`${textColor} ${buttonColor} p-1 font-bold rounded max-h-full text-sm`}\r\n            onClick={onEditClick}\r\n          />\r\n        ) : (\r\n          <ConfirmDiscardPair onDiscard={onDiscard} onConfirm={onConfirm} />\r\n        )}\r\n      </div>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default EditableValueRow;\r\n","/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/ImagePicker.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/config/paths.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/api/companies.js",["162"],"import { AUTHENTICATION_PATH } from '../config/paths'\r\nimport { authenticatedRequest } from '../helpers/AuthenticatedRequest'\r\n\r\nconst updateCompany = async(id, attrs) => {\r\n    \r\n    const response = await authenticatedRequest(`${AUTHENTICATION_PATH}/companies/${id}`, {\r\n        method: \"PUT\",\r\n        headers: {\r\n            \"Content-Type\": \"application/json\"\r\n        },\r\n        body: JSON.stringify(attrs)\r\n    })\r\n\r\n\r\n}\r\n\r\nconst createCompany = async (attrs) => {\r\n    const response = await authenticatedRequest(`${AUTHENTICATION_PATH}/companies`, {\r\n        method: \"POST\",\r\n        headers: {\r\n            \"Content-Type\": \"application/json\"\r\n        },\r\n        body: JSON.stringify(attrs)\r\n    })\r\n\r\n    return response\r\n}\r\n\r\nconst getCompanyByID = async (id) => {\r\n    if (!id) return\r\n    try {\r\n        const response = await authenticatedRequest(`${AUTHENTICATION_PATH}/companies/${id}`)\r\n        if (!response.ok) return\r\n        const company  = await response.json()\r\n        return company\r\n    } catch (error) {\r\n        console.log(error)\r\n    }\r\n}\r\n\r\n\r\nexport { updateCompany, createCompany, getCompanyByID }","/home/marianodsr/PersonalProjects/EXO/web-client/src/api/files.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/api/users.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/helpers/jwt.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/SignupForm/SignUpForm.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/InputWithIcon.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/CreateCompanyModal.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/MobileNavbar.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/ConfirmDiscardPair.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/EditButton.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/helpers/AuthenticatedRequest.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/SignupForm/Step2.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/SignupForm/Step1.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Containers/OrdersPage.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/Order.js",[],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/OrderList.js",["163"],"/home/marianodsr/PersonalProjects/EXO/web-client/src/Components/OrderContent.js",[],{"ruleId":"164","replacedBy":"165"},{"ruleId":"166","replacedBy":"167"},{"ruleId":"168","severity":1,"message":"169","line":22,"column":6,"nodeType":"170","endLine":22,"endColumn":26,"suggestions":"171"},{"ruleId":"172","severity":1,"message":"173",